pipeline {
    agent any

    stages {
        stage('pull') {
            steps {
              git branch: 'main', url: 'https://github.com/sayalithorat972-coder/github2-mirror-3.git'
            }
        }
        stage('build') {
            steps {
                echo 'building..'
            }
        }
        stage('test') {
            steps {
                echo 'testing....'
            }
        }
        stage('deploy') {
            steps {
                echo 'deploying....'
            }
        }
    }